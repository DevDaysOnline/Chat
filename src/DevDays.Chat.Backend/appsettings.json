{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KeyVault" : {
    "Uri" : "https://ddo-chat.vault.azure.net"
  },
  "Authentication" : {
    "Twitch" : {
      "Authority": "https://id.twitch.tv/oauth2",
      "ClientId": "YourClientId",
      "ClientSecret": "YourClientSecret",
      "Scope" : ["user:read:email", "openid"]
    }
  }
}
